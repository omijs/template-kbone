<import src="./template/subtree.wxml"/>
<import src="./template/subtree-cover.wxml"/>
<view
    wx:if="{{wxCompName === 'view'}}"
    class="{{class}}"
    style="{{style}}"
    hover-class="{{hoverClass}}"
    hover-stop-propagation="{{hoverStopPropagation}}"
    hover-start-time="{{hoverStayTime}}"
    hover-stay-time="{{hoverStayTime}}"
><template is="subtree" data="{{childNodes: innerChildNodes, inCover}}"/></view>
<image
    wx:elif="{{wxCompName === 'image'}}"
    class="{{class}}"
    style="{{style}}"
    rendering-mode="{{renderingMode}}"
    src="{{src}}"
    mode="{{mode}}"
    lazy-load="{{lazyLoad}}"
    show-menu-by-longpress="{{showMenuByLongpress}}"
    bindload="onImageLoad"
    binderror="onImageError"
></image>
<cover-image
    wx:elif="{{wxCompName === 'cover-image'}}"
    class="{{class}}"
    style="{{style}}"
    src="{{src}}"
    bindload="onCoverImageLoad"
    binderror="onCoverImageError"
></cover-image>
<input
    wx:elif="{{wxCompName === 'input'}}"
    class="{{class}}"
    style="{{style}}"
    value="{{value}}"
    type="{{type}}"
    password="{{password}}"
    placeholder="{{placeholder}}"
    placeholder-style="{{placeholderStyle}}"
    placeholder-class="{{placeholderClass}}"
    disabled="{{disabled}}"
    maxlength="{{maxlength}}"
    cursor-spacing="{{cursorSpacing}}"
    auto-focus="{{autoFocus}}"
    focus="{{focus}}"
    confirm-type="{{confirmType}}"
    confirm-hold="{{confirmHold}}"
    cursor="{{cursor}}"
    selection-start="{{selectionStart}}"
    selection-end="{{selectionEnd}}"
    adjust-position="{{adjustPosition}}"
    bindinput="onInputInput"
    bindfocus="onInputFocus"
    bindblur="onInputBlur"
    bindconfirm="onInputConfirm"
    bindkeyboardheightchange="onInputKeyBoardHeightChange"
></input>
<textarea
    wx:elif="{{wxCompName === 'textarea'}}"
    class="{{class}}"
    style="{{style}}"
    value="{{value}}"
    placeholder="{{placeholder}}"
    placeholder-style="{{placeholderStyle}}"
    placeholder-class="{{placeholderClass}}"
    disabled="{{disabled}}"
    maxlength="{{maxlength}}"
    auto-focus="{{autoFocus}}"
    focus="{{focus}}"
    auto-height="{{autoHeight}}"
    fixed="{{fixed}}"
    cursor-spacing="{{cursorSpacing}}"
    cursor="{{cursor}}"
    show-confirm-bar="{{showConfirmBar}}"
    selection-start="{{selectionStart}}"
    selection-end="{{selectionEnd}}"
    adjust-position="{{adjustPosition}}"
    bindfocus="onTextareaFocus"
    bindblur="onTextareaBlur"
    bindlinechange="onTextareaLineChange"
    bindinput="onTextareaInput"
    bindconfirm="onTextareaConfirm"
    bindkeyboardheightchange="onTextareaKeyBoardHeightChange"
></textarea>
<video 
    wx:elif="{{wxCompName === 'video'}}"
    class="{{class}}"
    style="{{style}}"
    src="{{src}}"
    duration="{{duration}}"
    controls="{{controls}}"
    danmu-list="{{danmuList}}"
    danmu-btn="{{danmuBtn}}"
    enable-danmu="{{enableDanmu}}"
    autoplay="{{autoplay}}"
    loop="{{loop}}"
    muted="{{muted}}"
    initial-time="{{initialTime}}"
    direction="{{direction}}"
    show-progress="{{showProgress}}"
    show-fullscreen-btn="{{showFullscreenBtn}}"
    show-play-btn="{{showPlayBtn}}"
    show-center-play-btn="{{showCenterPlayBtn}}"
    enable-progress-gesture="{{enableProgressGesture}}"
    object-fit="{{objectFit}}"
    poster="{{poster}}"
    show-mute-btn="{{showMuteBtn}}"
    title="{{title}}"
    play-btn-position="{{playBtnPosition}}"
    enable-play-gesture="{{enablePlayGesture}}"
    auto-pause-if-navigate="{{autoPauseIfNavigate}}"
    auto-pause-if-open-native="{{autoPauseIfOpenNative}}"
    vslide-gesture="{{vslideGesture}}"
    vslide-gesture-in-fullscreen="{{vslideGestureInFullscreen}}"
    bindplay="onVideoPlay"
    bindpause="onVideoPause"
    bindended="onVideoEnded"
    bindtimeupdate="onVideoTimeUpdate"
    bindfullscreenchange="onVideoFullScreenChange"
    bindwaiting="onVideoWaiting"
    binderror="onVideoError"
    bindprogress="onVideoProgress"
><template is="subtree" data="{{childNodes: innerChildNodes, inCover}}"/></video>
<button
    wx:elif="{{wxCompName === 'button'}}"
    class="{{class}}"
    style="{{style}}"
    size="{{size}}"
    type="{{type}}"
    plain="{{plain}}"
    disabled="{{disabled}}"
    loading="{{loading}}"
    form-type="{{formType}}"
    open-type="{{openType}}"
    hover-class="{{hoverClass}}"
    hover-stop-propagation="{{hoverStopPropagation}}"
    hover-start-time="{{hoverStartTime}}"
    hover-stay-time="{{hoverStayTime}}"
    lang="{{lang}}"
    session-from="{{sessionFrom}}"
    send-message-title="{{sendMessageTitle}}"
    send-message-path="{{sendMessagePath}}"
    send-message-img="{{sendMessageImg}}"
    app-parameter="{{appParameter}}"
    show-message-card="{{showMessageCard}}"
    bindgetuserinfo="onButtonGetUserInfo"
    bindcontact="onButtonContact"
    bindgetphonenumber="onButtonGetPhoneNumber"
    binderror="onButtonError"
    bindopensetting="onButtonOpenSetting"
    bindlaunchapp="onButtonLaunchApp"
><template is="subtree" data="{{childNodes: innerChildNodes, inCover}}"/></button>
<picker
    wx:elif="{{wxCompName === 'picker'}}"
    class="{{class}}"
    style="{{style}}"
    mode="{{mode}}"
    disabled="{{disabled}}"
    range="{{range}}"
    range-key="{{rangeKey}}"
    value="{{value}}"
    start="{{start}}"
    end="{{end}}"
    fields="{{fields}}"
    custom-item="{{customItem}}"
    bindchange="onPickerChange"
    bindcolumnchange="onPickerColumnChange"
    bindcancel="onPickerCancel"
><template is="subtree" data="{{childNodes: innerChildNodes, inCover}}"/></picker>
<live-player
    wx:elif="{{wxCompName === 'live-player'}}"
    class="{{class}}"
    style="{{style}}"
    src="{{src}}"
    mode="{{mode}}"
    autoplay="{{autoplay}}"
    muted="{{muted}}"
    orientation="{{orientation}}"
    object-fit="{{objectFit}}"
    background-mute="{{backgroundMute}}"
    min-cache="{{minCache}}"
    max-cache="{{maxCache}}"
    sound-mode="{{soundMode}}"
    auto-pause-if-navigate="{{autoPauseIfNavigate}}"
    auto-pause-if-open-native="{{autoPauseIfOpenNative}}"
    bindstatechange="onLivePlayerStateChange"
    bindfullscreenchange="onLivePlayerFullScreenChange"
    bindnetstatus="onLivePlayerNetStatus"
><template is="subtree-cover" data="{{childNodes: innerChildNodes}}"/></live-player>
<live-pusher
    wx:elif="{{wxCompName === 'live-pusher'}}"
    class="{{class}}"
    style="{{style}}"
    url="{{url}}"
    mode="{{mode}}"
    autopush="{{autopush}}"
    muted="{{muted}}"
    enable-camera="{{enableCamera}}"
    auto-focus="{{autoFocus}}"
    orientation="{{orientation}}"
    beauty="{{beauty}}"
    whiteness="{{whiteness}}"
    aspect="{{aspect}}"
    min-bitrate="{{minBitrate}}"
    max-bitrate="{{maxBitrate}}"
    waiting-image="{{waitingImage}}"
    waiting-image-hash="{{waitingImageHash}}"
    zoom="{{zoom}}"
    device-position="{{devicePosition}}"
    background-mute="{{backgroundMute}}"
    mirror="{{mirror}}"
    bindstatechange="onLivePusherStateChange"
    bindnetstatus="onLivePusherNetStatus"
    binderror="onLivePusherError"
    bindbgmstart="onLivePusherBgmStart"
    bindbgmprogress="onLivePusherBgmProgress"
    bindbgmcomplete="onLivePusherBgmComplete"
><template is="subtree-cover" data="{{childNodes: innerChildNodes}}"/></live-pusher>
<web-view
    wx:elif="{{wxCompName === 'web-view'}}"
    class="{{class}}"
    style="{{style}}"
    src="{{src}}"
    bindmessage="onWebviewMessage"
    bindload="onWebviewLoad"
    binderror="onWebviewError"
></web-view>
<map
    wx:elif="{{wxCompName === 'map'}}"
    class="{{class}}"
    style="{{style}}"
    longitude="{{longitude}}"
    latitude="{{latitude}}"
    scale="{{scale}}"
    markers="{{markers}}"
    polyline="{{polyline}}"
    circles="{{circles}}"
    controls="{{controls}}"
    include-points="{{includePoints}}"
    show-location="{{showLocation}}"
    polygons="{{polygons}}"
    subkey="{{subkey}}"
    layer-style="{{layerStyle}}"
    rotate="{{rotate}}"
    skew="{{skew}}"
    enable-3D="{{enable3D}}"
    show-compass="{{showCompass}}"
    enable-overlooking="{{enableOverlooking}}"
    enable-zoom="{{enableZoom}}"
    enable-scroll="{{enableScroll}}"
    enable-rotate="{{enableRotate}}"
    enable-satellite="{{enableSatellite}}"
    enable-traffic="{{enableTraffic}}"
    bindtap="onMapTap"
    bindmarkertap="onMapMarkerTap"
    bindcontroltap="onMapControlTap"
    bindcallouttap="onMapCalloutTap"
    bindupdated="onMapUpdated"
    bindregionchange="onMapRegionChange"
    bindpoitap="onMapPoiTap"
><template is="subtree-cover" data="{{childNodes: innerChildNodes}}"/></map>
<canvas
    wx:elif="{{wxCompName === 'canvas'}}"
    class="{{class}}"
    style="{{style}}"
    type="{{type}}"
    canvas-id="{{canvasId}}"
    disable-scroll="{{disableScroll}}"
    bindtouchstart="onTouchStart"
    bindtouchmove="onTouchMove"
    bindtouchend="onTouchEnd"
    bindtouchcancel="onTouchCancel"
    bindlongtap="onCanvasLongTap"
    binderror="onCanvasError"
><template is="subtree-cover" data="{{childNodes: innerChildNodes}}"/></canvas>
<cover-view
    wx:elif="{{wxCompName === 'cover-view'}}"
    class="{{class}}"
    style="{{style}}"
    scroll-top="{{scrollTop}}"
><template is="subtree-cover" data="{{childNodes: innerChildNodes}}"/></cover-view>
<block
    wx:elif="{{wxCompName === 'not-support'}}"
>{{content}}</block>
<template is="subtree" data="{{childNodes, inCover}}"/>